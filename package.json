{
  "name": "green-light",
  "displayName": "GreenLight",
  "description": "The GreenLight theme",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [ "onStartupFinished" ],
  "main": "./src/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "GreenLight Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/dark.json"
      }
    ],
    "iconThemes": [
			{
				"id": "greenlight-icons",
				"label": "GreenLight Icons",
				"path": "./icons/icons.json"
			}
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  }
}
